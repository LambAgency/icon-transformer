{
  "input": "./input",
  "output": "./output",
  "classify": [
    {
      "key": "fill",
      "maps": [
        {
          "class": "icon-primary",
          "values": ["#4A4A4A", "rgb(74,74,74)"]
        },
        {
          "class": "icon-secondary",
          "values": ["#FF9859", "rgb(255,152,89)"]
        },
        {
          "class": "icon-empty",
          "values": ["none"]
        }
      ]
    }
  ],
  "svgo": {
    "plugins": [
      {
        "removeAttrs": {
          "attrs": "stroke"
        }
      },
      { "removeViewBox": false },
      { "removeStyleElement": true },
      { "removeScriptElement": true }
    ]
  },
  "cleanPrefixes": ["icons_", "example-"]
}
