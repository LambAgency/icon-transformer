{
  "input": "./input",
  "output": "./output",
  "classify": [
    {
      "attribute": "fill",
      "maps": [
        {
          "class": "icon-white",
          "values": [
            "#fff",
            "#ffffff",
            "#FFF",
            "#FFFFFF",
            "white",
            "rgb(255,255,255)"
          ]
        },
        {
          "class": "icon-empty",
          "values": ["none"]
        }
      ]
    }
  ],
  "svgo": {
    "plugins": [
      {
        "removeAttrs": {
          "attrs": "stroke"
        }
      },
      { "removeViewBox": false },
      { "removeStyleElement": true },
      { "removeScriptElement": true }
    ]
  },
  "cleanPrefixes": ["icons_", "example-"],
  "outputExtension": ".svg"
}
